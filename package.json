{
  "name": "godtiercodereviewer",
  "displayName": "godtiercodereviewer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "type": "commonjs",
  "activationEvents": [
    "onCommand:godtiercodereviewer.start",
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "godtiercodereviewer.start",
        "title": "Start Code Review"
      }
    ],
    "configuration": {
      "title": "God Tier Code Reviewer",
      "properties": {
        "godtiercodereviewer.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your Google Generative AI (Gemini) API Key."
        }
      }
    },
    "codeActions": [
      {
        "languages": [
          "javascript"
        ],
        "kind": "quickfix"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.54.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "@babel/generator": "^7.27.5",
    "@babel/parser": "^7.27.7",
    "@google/genai": "^1.15.0",
    "@google/generative-ai": "^0.24.1",
    "diff": "^8.0.2",
    "dotenv": "^17.2.2",
    "recast": "^0.23.11"
  }
}
