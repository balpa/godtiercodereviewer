{
  "name": "godtiercodereviewer",
  "displayName": "godtiercodereviewer",
  "description": "",
  "version": "0.0.2",
  "publisher": "balpa",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "type": "commonjs",
  "activationEvents": [
    "onCommand:godtiercodereviewer.start",
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "godtiercodereviewer.start",
        "title": "God Tier: Kodu ƒ∞ncele"
      },
      {
        "command": "godtiercodereviewer.applySuggestionAtCursor",
        "title": "‚úÖ God Tier: √ñneriyi Uygula"
      },
      {
        "command": "godtiercodereviewer.showDiffAtCursor",
        "title": "üëÄ God Tier: Deƒüi≈üiklikleri G√∂r"
      },
      {
        "command": "godtiercodereviewer.rejectSuggestionAtCursor",
        "title": "‚ùå God Tier: √ñneriyi Reddet"
      },
      {
        "command": "godtier.applyFromCodeLens",
        "title": "God Tier (CodeLens): Uygula"
      },
      {
        "command": "godtier.showDiffFromCodeLens",
        "title": "God Tier (CodeLens): Farkƒ± G√∂r"
      },
      {
        "command": "godtier.rejectFromCodeLens",
        "title": "God Tier (CodeLens): Reddet"
      }
    ],
    "submenus": [
      {
        "id": "godtier.contextMenu",
        "label": "God Tier √ñnerisi"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "godtier.contextMenu",
          "group": "1_modification@1",
          "when": "editorTextFocus && resourceLangId == 'javascript' || resourceLangId == 'typescript' || resourceLangId == 'javascriptreact' || resourceLangId == 'typescriptreact'"
        }
      ],
      "godtier.contextMenu": [
        {
          "command": "godtiercodereviewer.applySuggestionAtCursor",
          "group": "1_apply"
        },
        {
          "command": "godtiercodereviewer.showDiffAtCursor",
          "group": "1_apply"
        },
        {
          "command": "godtiercodereviewer.rejectSuggestionAtCursor",
          "group": "2_reject"
        }
      ]
    },
    "configuration": {
      "title": "God Tier Code Reviewer",
      "properties": {
        "godtiercodereviewer.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your Google Generative AI (Gemini) API Key."
        }
      }
    },
    "codeActions": [
      {
        "languages": [
          "javascript"
        ],
        "kind": "quickfix"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.54.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "@babel/generator": "^7.27.5",
    "@babel/parser": "^7.27.7",
    "@google/genai": "^1.15.0",
    "@google/generative-ai": "^0.24.1",
    "diff": "^8.0.2",
    "dotenv": "^17.2.2",
    "prettier": "^3.6.2",
    "recast": "^0.23.11"
  }
}
