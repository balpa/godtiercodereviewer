{
  "name": "godtiercodereviewer",
  "displayName": "godtiercodereviewer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:godtiercodereviewer.start",
    "onCommand:godtiercodereviewer.applyFix",
    "onCommand:godtiercodereviewer.rejectFix",
    "onLanguage:javascript"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "godtiercodereviewer.start",
        "title": "Start Code Review"
      },
      {
        "command": "godtiercodereviewer.applyFix",
        "title": "Apply Fix"
      },
      {
        "command": "godtiercodereviewer.rejectFix",
        "title": "Reject Fix"
      }
    ],
    "codeActions": [
      {
        "languages": ["javascript"],
        "kind": "quickfix"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.54.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
